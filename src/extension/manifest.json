{
    "name": "Speechmote",
    "description": "Speech to emoji extension for assistive technology users.",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "permissions": ["contextMenus","tabs"],
    "commands": {
        "stop" : {
            "suggested_key": {
                 "default": "Alt+Shift+S" 
            },
            "description": "Stop Recording"
        }
    }
}